{
  "name": "er-api-sdk",
  "version": "1.0.3",
  "description": "A Lightweight Typescript/Javascript SDK for AI, media, games, and utilities",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "prepare": "npm run build",
    "docs": "typedoc --out docs src/index.ts",
    "docs:dev": "typedoc --out docs src/index.ts --watch",
    "test": "jest"
  },
  "keywords": [
    "er-api",
    "sdk",
    "ai",
    "typescript",
    "api-client",
    "ai-sdk",
    "ytmp3"
  ],
  "author": "ErNewDev0",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/ErBots/Er-Api-Sdk"
  },
  "bugs": {
    "url": "https://github.com/ErBots/Er-Api-Sdk/issues"
  },
  "homepage": "https://ErNewDev0.github.io",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.8.0",
    "typedoc": "^0.25.12",
    "rimraf": "^5.0.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "@typescript-eslint/parser": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}